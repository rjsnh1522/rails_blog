<h1 id="page_title">Stuff I've Built</h1>
<div class="skinny_wrapper" id="posts_wrapper">

  <% @projects.each do |project|%>
      <div class="post">
        <p class="date">
          <%= project.created_at.strftime('%A, %B %d')%>
        <h2><a href="#"><%= link_to project.title, project%></a></h2>
        </p>
        <hr>
      </div>
  <% end %>

</div>